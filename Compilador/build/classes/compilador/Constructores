/**
     * Default constructor.
     */
    public parser() throws IOException {
        super();
        this.bw = new BufferedWriter(new FileWriter(archivoPrograma));
        bw.write("package compilador;\n\npublic class " + nombrePrograma + "{\n\n");
    }

    /**
     * Constructor which sets the default scanner.
     */
    public parser(java_cup.runtime.Scanner s) throws IOException {
        super(s);
        this.bw = new BufferedWriter(new FileWriter(archivoPrograma));
        bw.write("package compilador;\n\npublic class " + nombrePrograma + "{\n\n");
    }

    /**
     * Constructor which sets the default scanner.
     */
    public parser(java_cup.runtime.Scanner s, java_cup.runtime.SymbolFactory sf) throws IOException {
        super(s, sf);
        this.bw = new BufferedWriter(new FileWriter(archivoPrograma));
        bw.write("package compilador;\n\npublic class " + nombrePrograma + "{\n\n");
    }
